        required
      />
    </div>
    <button class="btn btn-success mt-3 mb-3">Ingresar</button>
  </form>
  <form action="/signup" method="get">
    <button id="buttonLogOut" class="btn btn-warning">Ir a registrarse</button>
  </form>
</div>