<div class="mt-3 col">
    <h1 class="text-center" style="color:blue;">Productos random con Faker.js</h1>
    {{#if productos}}
    <table class="table table-dark mt-3">
        <tr style="color: yellow;">
            <th>Nombre</th>
            <th>Precio</th>
            <th>Link</th>
        </tr>
        {{#each productos}}
        <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.precio}}</td>
            <td>
                <img style="height: 2rem; width: 2rem;" src={{this.foto}} alt={{this.nombre}}>
            </td>
        </tr>
        {{/each}}
    </table>
    {{else}}
    <h1 class="text-center" style="color:blue;">No hay productos</h1>
    {{/if}}
</div>
</div>