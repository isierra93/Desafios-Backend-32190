  <form action="/signup" method="post">
    <div class="form-group mb-2">
      <label for="username">Email</label>
      <input
        type="text"
        id="username"
        name="username"
        class="form-control"
        required
      />
    </div>
    <div class="form-group mb-2">
      <label for="password">ContraseÃ±a</label>
      <input
        type="text"
        id="password"
