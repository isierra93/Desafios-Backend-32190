        required
      />
    </div>
    <button type="submit" class="btn btn-success mt-3 mb-3">Registrar</button>
  </form>
  <form action="/login" method="get">
    <button id="buttonLogOut" class="btn btn-warning">Ir a iniciar sesion</button>
  </form>
</div>